{
    "$schema": "./schemas/mobile-schema.json",
    "identifier": "mobile-capability",
    "windows": [
        "main"
    ],
    "platforms": [
        "iOS",
        "android"
    ],
    "permissions": [
        "store:allow-set",
        "store:allow-save",
        "store:allow-get",
        "store:allow-delete",
        "http:allow-fetch",
        "http:default"
    ]
}